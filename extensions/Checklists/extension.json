	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Checklists/resources"
	},
	"Hooks": {
		"BeforePageDisplay": "Checklists\\Hook\\BeforePageDisplay\\AddModules::callback",
		"ParserFirstCallInit": "\\BlueSpice\\Checklists\\Hook\\ParserFirstCallInit\\Parse::callback"
	},
	"manifest_version": 2
